.radio-grid-box {
  display: flex;
  justify-content: space-between;
  flex-wrap: wrap;
}
.radio-grid-item {
  width: 160rpx;
  min-height: 74rpx;
  border: 1rpx solid #ccc;
  border-radius: 4rpx;
  line-height: 74rpx;
  text-align: center;
  font-size: 30rpx;
  color: #999;
  margin-bottom: 20rpx;
}
.select .radio-grid-item {
  background: #108EE9;
  color: #fff;
  border-color: #108EE9;
}